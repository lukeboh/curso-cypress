{
  "name": "curso-cypress",
  "version": "1.0.0",
  "description": "",
  "license": "ISC",
  "author": "",
  "type": "commonjs",
  "main": "index.js",
  "scripts": {
    "clean": "rimraf cypress/results",
    "runz": "cypress run",
    "report:merge": "npx mochawesome-merge cypress/results/mochawesome-report/*.json > cypress/results/mochawesome-merge.json",
    "report:generate": "npx mochawesome-report-generator cypress/results/mochawesome-merge.json --reportDir cypress/results --charts --inline --timestamp=yyyymmdd-HHMMss",
    "report": "npm run runz ; npm run report:merge ; npm run report:generate",
    "all": "npm run clean ; npm run report"
  },
  "devDependencies": {
    "cypress": "^13.6.4",
    "cypress-multi-reporters": "^2.0.5",
    "mochawesome": "^7.1.3",
    "mochawesome-merge": "^4.4.1",
    "mochawesome-report-generator": "^6.2.0",
    "rimraf": "^6.0.1"
  }
}
